#!/bin/sh
cd "t e s t"
chmod +x HelloIntelAsm.asm
chmod +x HelloArmAsm.s
chmod +x HelloC.c
chmod +x HelloCpp.cpp
chmod +x HelloCS.cs
chmod +x HelloVBNet.vb
chmod +x HelloGo.go
chmod +x HelloRust.rs
chmod +x HelloJava.java
chmod +x HelloKotlin.kt
chmod +x HelloKotlinScript.kts
chmod +x HelloTS.ts
chmod +x HelloJS.js
chmod +x HelloPy.py
chmod +x HelloSh.sh
chmod +x HelloOCaml.ml
chmod +x HelloAWK.awk
chmod +x HelloPerl.pl
chmod +x HelloOctave.m
chmod +x HelloGenie.gs
chmod +x HelloVala.vala
chmod +x HelloTS2.ts
chmod +x ./G\ U\ I/HelloValaGtk.vala

./HelloIntelAsm.asm
./HelloArmAsm.s
./HelloC.c
./HelloCpp.cpp
./HelloCS.cs
./HelloVBNet.vb
./HelloGo.go
./HelloRust.rs
./HelloJava.java
./HelloKotlin.kt
./HelloKotlinScript.kts
./HelloTS.ts
./HelloJS.js
./HelloPy.py
./HelloSh.sh
./HelloOCaml.ml
./HelloAWK.awk
./HelloPerl.pl
./HelloOctave.m 2> /dev/null
./HelloGenie.gs
./HelloVala.vala
./HelloTS2.ts -f 3 -t 5
./G\ U\ I/HelloValaGtk.vala 2> /dev/null

execute -r Hello vala 'print (@"$(args[1]) from VALA\n")'
execute -r Hello c++ 'cout << argv[1] << " from CPP" << endl'
execute -r "1 1" c 'int i=atoi(argv[1]),j=atoi(argv[2]),k=i+j;printf("%d + %d = %d\n",i,j,k)'
execute vala 'Gtk.init(ref args);var w=new Gtk.Window();w.title="Ello";w.set_default_size(300,150);w.border_width=10;w.destroy.connect(Gtk.main_quit);var b=new Gtk.Button();b.label="CLICK ME!";b.clicked.connect(()=>{print("clicked\n");Gtk.main_quit();});w.add(b);w.show_all();Gtk.main()'


