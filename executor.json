{
    ".java": {
        "name": "Java",
        "outFile": "'{outPath}{fileNameNoExt}.class'",
        "cm": "javac {inputFiles} && cd '{filePath}' && mv {fileNameNoExt}*.class '{outPath}'",
        "rn": "java -cp '{outPath}' {fileNameNoExt}",
        "tmp": "public class Tmp {{ public static void main(String[] args) {{ {s}; }} }}",
        "needs": [ "javac", "java" ],
        "install": {
            "arch": "sudo pacman -S jdk8-openjdk",
            "fedora": "sudo dnf install java-openjdk",
            "ubuntu": "sudo apt-get install openjdk-8-jre openjdk-8-jdk-headless"
        }
    },
    ".kt": {
        "name": "Kotlin",
        "outFile": "'{outPath}{fileNameNoExt}.jar'",
        "cm": "kotlinc {inputFiles} -include-runtime -d {outFile}",
        "rn": "cd \"{outPath}\" && java -jar {outFile}",
        "tmp": "fun main(args: Array<String>) {{ {s} }}",
        "needs": [ "kotlinc", "java" ],
        "install": {
            "arch": "sudo pacman -S kotlin",
            "default": "echo \"$(curl -s https://get.sdkman.io) && source \\\"\\${SDKMAN_DIR}/bin/sdkman-init.sh\\\" && sdk install kotlin\" | bash"
        }
    },
    ".kts": {
        "name": "Kotlin Script",
        "outFile": "'{sourceFile}'",
        "cm": "",
        "rn": "kotlinc -script {outFile}",
        "needs": [ "kotlinc", "java" ],
        "install": {
            "arch": "sudo pacman -S kotlin",
            "default": "echo \"$(curl -s https://get.sdkman.io) && source \\\"\\${SDKMAN_DIR}/bin/sdkman-init.sh\\\" && sdk install kotlin\" | bash"
        }
    },
    ".vala": {
        "name": "Vala",
        "outFile": "'{outPath}{fileName}.bin'",
        "cm": "valac {inputFiles} -o {outFile}",
        "rn": "{outFile}",
        "tmp": "void main (string[] args) {{ {s}; }}",
        "needs": [ "valac" ],
        "install": {
            "arch": "sudo pacman -S vala",
            "fedora": "sudo dnf install vala",
            "ubuntu": "sudo add-apt-repository ppa:vala-team && sudo apt-get update && sudo apt-get install -y valac"
        },
        "cm_args": {
            "using Gtk|Gtk\\.": "--pkg gtk+-3.0",
            "File\\.| async ": "--pkg gio-2.0",
            "using Gee|Gee\\.": "--pkg gee-0.8",
            "Thread\\.": "--thread"
        }
    },
    ".gs": {
        "equal": ".vala"
    },
    ".ml": {
        "name": "OCaml",
        "outFile": "'{outPath}{fileName}.bin'",
        "cm": "ocamlopt {inputFiles} -o {outFile} && rm -f '{filePath}{fileNameNoExt}.o' '{filePath}{fileNameNoExt}.cmi' '{filePath}{fileNameNoExt}.cmx'",
        "rn": "{outFile}",
        "needs": [ "ocamlopt" ],
        "install": {
            "arch": "sudo pacman -S ocaml",
            "fedora": "sudo dnf install ocaml",
            "ubuntu": "sudo apt-get install ocaml"
        }
    },
    ".c": {
        "name": "C",
        "outFile": "'{outPath}{fileName}.bin'",
        "cm": "gcc {inputFiles} -o {outFile}",
        "rn": "{outFile}",
        "tmp": "#include <stdio.h>\n#include <stdlib.h>\nint main(int argc, char** argv) {{\n {s}; return 0; }}",
        "needs": [ "gcc" ],
        "install": {
            "arch": "sudo pacman -S gcc",
            "fedora": "sudo dnf install -y gcc",
            "ubuntu": "sudo apt-get install -y build-essential"
        },
        "cm_args": {
            "#include\\s*<math\\.h>": "-lm",
            "#include\\s*<pthread\\.h>": "-pthread",
            "#include\\s*<X11.*>": "-lX11",
            "#include\\s*<glib\\.h>": "`pkg-config glib-2.0 --cflags --libs`",
            "#include\\s*<gtk/gtk\\.h>": "`pkg-config gtk+-3.0 --cflags --libs`"
        }
    },
    ".cpp": {
        "name": "C++",
        "outFile": "'{outPath}{fileName}.bin'",
        "cm": "g++ {inputFiles} -o {outFile}",
        "rn": "{outFile}",
        "tmp": "#include<iostream>\nusing namespace std;int main(int argc, char* argv[]) {{ {s}; return 0; }}",
        "needs": [ "g++" ],
        "install": {
            "arch": "sudo pacman -S gcc",
            "fedora": "sudo dnf install gcc-c++",
            "ubuntu": "sudo apt-get install g++"
        },
        "cm_args": {
            "#include\\s*<thread>": "-pthread",
            "#include\\s*<gtkmm": "`pkg-config gtkmm-3.0 --cflags --libs`"
        }
    },
    ".C": {
        "equal": ".cpp"
    },
    ".cc": {
        "equal": ".cpp"
    },
    ".CPP": {
        "equal": ".cpp"
    },
    ".c++": {
        "equal": ".cpp"
    },
    ".cp": {
        "equal": ".cpp"
    },
    ".cxx": {
        "equal": ".cpp"
    },
    ".cs": {
        "name": "C#",
        "outFile": "'{outPath}{fileNameNoExt}.exe'",
        "cm": "mcs {inputFiles} -out:{outFile}",
        "rn": "mono {outFile}",
        "tmp": "using System; public class Tmp {{ static public void Main (string[] args) {{ {s}; }} }}",
        "needs": [ "mcs", "mono" ],
        "install": {
            "arch": "sudo pacman -S mono",
            "fedora": "sudo dnf install mono-devel",
            "ubuntu": "sudo apt-get install mono-complete"
        },
        "cm_args": {
            "using Gtk": "-pkg:gtk-sharp-3.0"
        }
    },
    ".vb": {
        "name": "VB.Net",
        "outFile": "'{outPath}{fileNameNoExt}.exe'",
        "cm": "vbnc {inputFiles} 1>/dev/null /out:{outFile}",
        "rn": "mono {outFile}",
        "tmp": "Imports System\nPublic Module modmain\nSub Main()\n{s}\nEnd Sub\nEnd Module",
        "needs": [ "vbnc", "mono" ],
        "install": {
            "fedora": "sudo dnf install mono-devel",
            "ubuntu": "sudo apt-get install mono-complete"
        }
    },
    ".rs": {
        "name": "Rust",
        "outFile": "'{outPath}{fileName}.bin'",
        "cm": "rustc {inputFiles} -o {outFile}",
        "rn": "{outFile}",
        "tmp": "fn main() {{ {s}; }}",
        "needs": [ "rustc" ],
        "install": {
            "arch": "sudo pacman -S rust",
            "default": "curl https://sh.rustup.rs -sSf | sh && bash -c \"if ! grep -q 'source \\$HOME/.cargo/env' \\$HOME/.bashrc; then echo 'source \\$HOME/.cargo/env' >> \\$HOME/.bashrc; fi\""
        }
    },
    ".go": {
        "name": "Go",
        "outFile": "'{outPath}{fileName}.bin'",
        "cm": "go build -o {outFile} {inputFiles}",
        "rn": "{outFile}",
        "tmp": "package main\nimport \"fmt\"\nfunc main() {{ {s} }}",
        "needs": [ "go" ],
        "install": {
            "arch": "sudo pacman -S go",
            "fedora": "sudo dnf install golang",
            "ubuntu": "sudo apt-get install golang"
        }
    },
    ".js": {
        "name": "JavaScript",
        "outFile": "'{sourceFile}'",
        "cm": "",
        "rn": "node {outFile}",
        "needs": [ "node" ],
        "install": {
            "arch": "sudo pacman -S nodejs",
            "fedora": "sudo dnf install nodejs",
            "ubuntu": "sudo apt-get install nodejs"
        }
    },
    ".ts": {
        "name": "TypeScript",
        "outFile": "'{outPath}{fileNameNoExt}.js'",
        "cm": "tsc {inputFiles} && mv '{filePath}{fileNameNoExt}.js' '{outPath}'",
        "rn": "node {outFile}",
        "needs": [ "tsc" ],
        "install": {
            "arch": "sudo pacman --needed -S nodejs npm && sudo npm install -g typescript",
            "fedora": "sudo dnf install nodejs npm && sudo npm install -g typescript",
            "ubuntu": "sudo apt-get install nodejs npm && sudo npm install -g typescript"
        }
    },
    ".asm": {
        "name": "Intel Assembly",
        "outFile": "'{outPath}{fileName}.bin'",
        "cm": "nasm {inputFiles} -o '{outPath}{fileName}.o' -f elf{arch} && ld -s -o {outFile} '{outPath}{fileName}.o' && rm '{outPath}{fileName}.o'",
        "rn": "{outFile}",
        "needs": [ "nasm" ],
        "install": {
            "arch": "sudo pacman -S nasm",
            "fedora": "sudo dnf install nasm",
            "ubuntu": "sudo apt-get install nasm"
        }
    },
    ".s": {
        "name": "ARM Assembly",
        "outFile": "'{outPath}{fileName}.bin'",
        "cm": "arm-linux-gnu-as {inputFiles} -o '{outPath}{fileName}.o' && arm-linux-gnu-ld -s -o {outFile} '{outPath}{fileName}.o'",
        "rn": "qemu-arm {outFile}",
        "needs": [ "arm-linux-gnu-as", "arm-linux-gnu-ld", "qemu-arm" ],
        "install": {
            "arch": "sudo pacman --needed -S arm-none-eabi-gcc arm-none-eabi-newlib qemu-arch-extra && sudo ln -s /usr/bin/arm-none-eabi-as /usr/bin/arm-linux-gnu-as && sudo ln -s /usr/bin/arm-none-eabi-ld /usr/bin/arm-linux-gnu-ld",
            "fedora": "sudo dnf install gcc-arm-linux-gnu qemu-system-arm",
            "ubuntu": "sudo apt-get install gcc-arm-linux-gnueabi qemu-user && sudo ln -s /usr/bin/arm-linux-gnuabi-as /usr/bin/arm-linux-gnu-as && sudo ln -s /usr/bin/arm-linux-gnuabi-ld /usr/bin/arm-linux-gnu-ld"
        }
    },
    ".py": {
        "name": "Python",
        "outFile": "'{sourceFile}'",
        "cm": "",
        "rn": "python3 {outFile}",
        "needs": [ "python3" ],
        "install": {
            "arch": "sudo pacman -S python3",
            "fedora": "sudo dnf install -y python3",
            "ubuntu": "sudo apt-get install -y python3"
        }
    },
    ".m": {
        "name": "Octave",
        "outFile": "'{sourceFile}'",
        "cm": "",
        "rn": "octave --persist --no-gui --quiet {outFile}",
        "needs": [ "octave" ],
        "install": {
            "arch": "sudo pacman -S octave",
            "fedora": "sudo dnf install octave",
            "ubuntu": "sudo apt-get install octave"
        }
    },
    ".sh": {
        "name": "Shell",
        "outFile": "'{sourceFile}'",
        "cm": "",
        "rn": "bash {outFile}",
        "needs": [ "bash" ]
    },
    ".pl": {
        "name": "Perl",
        "outFile": "'{outPath}{fileName}'",
        "cm": "cp {inputFiles} '{outPath}'",
        "rn": "perl {outFile}",
        "needs": [ "perl" ],
        "install": {
            "arch": "sudo pacman -S perl",
            "fedora": "sudo dnf install perl",
            "ubuntu": "sudo apt-get install perl"
        }
    },
    ".awk": {
        "name": "AWK",
        "outFile": "'{sourceFile}'",
        "cm": "",
        "rn": "awk -f {outFile}",
        "needs": [ "awk" ]
    },
    ".r": {
        "name": "R",
        "outFile": "'{sourceFile}'",
        "cm": "",
        "rn": "R --quiet -f {outFile}",
        "needs": [ "R" ],
        "install": {
            "arch": "sudo pacman -S r"
        }
    },
    ".dart": {
        "name": "Dart",
        "outFile": "'{sourceFile}'",
        "cm": "",
        "rn": "dart {outFile}",
        "needs": [ "dart" ],
        "install": {
            "arch": "sudo pacman -S dart"
        }
    },
    ".jl": {
        "name": "Julia",
	      "outFile": "'{sourceFile}'",
        "cm": "",
        "rn": "julia {outFile}",
        "needs": [ "julia" ],
        "install": {
            "arch": "sudo pacman -S julia"
        }
    },
    ".hx": {
        "name": "Haxe",
	      "outFile": "'{outPath}{fileNameNoExt}.py'",
        "cm": "cd '{filePath}' && haxe -main {fileNameNoExt} -python {outFile} -dce full",
        "rn": "python3 {outFile}",
        "needs": [ "haxe", "python3" ],
        "install": {
            "arch": "sudo pacman --needed -S haxe python3"
        }
    }
}
